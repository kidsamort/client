{
  "root": true,
  "extends": [
      
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "project": ["./tsconfig.json"] },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "off",
      "@typescript-eslint/strict-boolean-expressions": [
          2,
          {
              "allowString" : false,
              "allowNumber" : false
          }
      ]
  },
  "ignorePatterns": ["src/**/*.test.ts", "src/frontend/generated/*"]
}