{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss",
        "props": "templates/component/TemplateName.props.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  },
  "page": {
    "path": "src/pages",
    "withStyle": true,
    "withTest": false,
    "withStory": false,
    "withLazy": false
  }
}

